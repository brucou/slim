{
  "name": "yed2kingly",
  "version": "0.1.0",
  "author": "brucou",
  "description": "Supports the conversion of graphs drawn with the yed editor into Kingly state machines",
  "devDependencies": {
    "assert-diff": "^3.0.0",
    "chai": "^4.2.0",
    "mocha": "^7.1.1"
  },
  "dependencies": {
    "funfix": "^7.0.1",
    "ejs": "^3.1.2",
    "commander": "^5.0.0",
    "fs": "latest",
    "prettier": "^2.0.4",
    "fast-xml-parser": "^3.16.0",
